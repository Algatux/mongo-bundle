<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="facile_mongo_db.data_collector"
                 class="Facile\MongoDbBundle\DataCollector\MongoDbDataCollector"
                 public="false">
            <call method="setLogger">
                <argument type="service" id="facile_mongo_db.logger" />
            </call>
            <tag name="data_collector" template="FacileMongoDbBundle:Collector:mongo.html.twig" id="mongodb" priority="250" />
        </service>

    </services>

</container>
